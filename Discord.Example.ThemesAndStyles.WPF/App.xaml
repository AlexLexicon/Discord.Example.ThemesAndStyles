<Application 
    x:Class="Discord.Example.ThemesAndStyles.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Discord.Example.ThemesAndStyles.WPF;component/Colors.xaml"/>
                <ResourceDictionary Source="/Discord.Example.ThemesAndStyles.WPF;component/Styles/MyButtonStyle.xaml"/>
                <ResourceDictionary Source="/Discord.Example.ThemesAndStyles.WPF;component/Styles/MyComboBoxStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
