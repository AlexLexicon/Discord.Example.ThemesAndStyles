<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Light_Background_Normal" Color="#FFF4F4F4"/>
    <SolidColorBrush x:Key="Light_Background_MouseOver" Color="#FFE5E5E5"/>
    <SolidColorBrush x:Key="Light_Background_Pressed" Color="#FFD6D6D6"/>

    <SolidColorBrush x:Key="Light_Foreground_Normal" Color="#FF000000"/>
    <SolidColorBrush x:Key="Light_Foreground_MouseOver" Color="#FF000000"/>
    <SolidColorBrush x:Key="Light_Foreground_Pressed" Color="#FF000000"/>

    <SolidColorBrush x:Key="Light_Border_Brush" Color="#FFBFBFBF"/>


    <SolidColorBrush x:Key="Dark_Background_Normal" Color="#FF5C5C5C"/>
    <SolidColorBrush x:Key="Dark_Background_MouseOver" Color="#FF6D6D6D"/>
    <SolidColorBrush x:Key="Dark_Background_Pressed" Color="#FF8B8B8B"/>

    <SolidColorBrush x:Key="Dark_Foreground_Normal" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="Dark_Foreground_MouseOver" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="Dark_Foreground_Pressed" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="Dark_Border_Brush" Color="#FF8F8F8F"/>

</ResourceDictionary>